{
  "ConnectionStrings": {
    "Default": "{Default_Connection_String}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "JWTConfig": {
    "Key": "{Private_Hash_Key}",
    "Audience": "Audience",
    "Issuer": "Issuer",
    "ExpiresInHours": 2
  }
}
